<header class='header'>
  {{#with header}}

    <nav class='header_nav'>
      <a href='#test' class='brandName'>
        {{brandName}}
      </a>
      <div class='header_nav_right'>
        {{#each links}}
          <a href='{{url}}' class='nav_link'>{{text}}</a>
        {{/each}}
      </div>
    </nav>

    <div class='header_ad'>
      <div class='text_ad'>
        <span class='ad_logo'>Ad</span>
        <div class='text_ad_text'>{{ad.text}}:
          {{ad.url}}</div>
      </div>
      <div class='userOnline'>User online:
        {{userOnline}}</div>
    </div>
  {{/with}}
</header>